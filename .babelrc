{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-syntax-decorators",
      {
        "legacy": true
      }
    ],
    "@babel/plugin-syntax-dynamic-import",
    [
      "babel-plugin-styled-components",
      {
        "fileName": false,
        "transpileTemplateLiterals": true
      }
    ],
    "lodash",
    [
      "import",
      {
        "libraryDirectory": "es",
        "libraryName": "antd",
        "style": true
      },
      "antd"
    ],
    [
      "import",
      {
        "libraryDirectory": "es",
        "libraryName": "antd-mobile",
        "style": true
      },
      "antd-mobile"
    ]
  ]
}